{
  "name": "regal-steward",
  "version": "0.1.0",
  "description": "Emporium Steward for Discord",
  "main": "app.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts"
    }
  },
  "scripts": {
    "test": "node --watch ./dist/app.js",
    "deploy-commands": "node deploy-commands.js",
    "build": "tsc && npm run copy:assets & npm run copy:env",
    "copy:assets": "cp -R src/assets dist/",
    "copy:env": "cp .env dist/",
    "watch": "npm-watch"
  },
  "keywords": [
    "discord",
    "bot",
    "automation",
    "utility"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.1.0",
    "@discordjs/voice": "^0.17.0",
    "@distube/ytsr": "^2.0.4",
    "@napi-rs/canvas": "^0.1.52",
    "@types/node": "^20.12.11",
    "@types/sqlite3": "^3.1.11",
    "discord-interactions": "^3.2.0",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "nanoid": "^5.0.7",
    "node-fetch": "^3.2.3",
    "rxjs": "^7.8.1",
    "sodium": "^3.0.2",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.1",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "npm-watch": "^0.13.0"
  }
}
